$("a[data-title='computing']").click( function() {
	dataLayer.push({
		'event': 'eventGA',
		'eventCategory': 'Scholarship Pages', 
		'eventAction': 'Internal Link Clicked', 
		'eventLabel': 'Computing',
		'eventValue': '0', 
		'eventNonInteraction': 'false'
	});
});

